#include <stdio.h>
#include <canio.h>
{
    int a,b,c
    printf("enter 1st side length of triangle");
    scanf("%d,"&a);
    printf("enter 2nd side length of trianle");
    scanf("%d",&b);
    printf("enter 3rd side length of triangle");
    scanf("%d",&c);
    if(a==b&&a==c)
{
    printf ("triangle is equilateral");
    break;
}
    else if(a==b|| a==c||b==c)
{
    printf("triangle is isoscales");
    break;
}
    else
{
    printf("triangle is scalene");
}
}